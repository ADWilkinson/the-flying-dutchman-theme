{
    "name": "The Flying Dutchman Soft",
    "author": "Created for those who prefer softer, muted nautical colors",
    "type": "dark",
    "semanticTokenColors": {
        "parameter.declaration": {
            "foreground": "#c8a96c"
        },
        "parameter": {
            "foreground": "#a8b8c8"
        },
        "property.declaration": {
            "foreground": "#5da6a3"
        },
        "property.defaultLibrary": {
            "foreground": "#6ba3d0"
        },
        "*.defaultLibrary": {
            "foreground": "#6ba3d0"
        },
        "variable.defaultLibrary": {
            "foreground": "#6ba3d0"
        },
        "variable.declaration": {
            "foreground": "#88a2b4"
        },
        "variable": {
            "foreground": "#a8b8c8"
        },
        "method": {
            "foreground": "#6b9dc4"
        },
        "method.declaration": {
            "foreground": "#6b9dc4"
        },
        "method.defaultLibrary": {
            "foreground": "#5da6c7"
        },
        "function": {
            "foreground": "#6ba3d0"
        },
        "function.defaultLibrary": {
            "foreground": "#6ba3d0"
        },
        "macro": {
            "foreground": "#6b9dc4"
        },
        "type": {
            "foreground": "#c8a96c"
        },
        "type.defaultLibrary": {
            "foreground": "#d4b574"
        },
        "class": {
            "foreground": "#c8a96c"
        },
        "class.defaultLibrary": {
            "foreground": "#d4b574"
        },
        "interface": {
            "foreground": "#c8a96c"
        },
        "enum": {
            "foreground": "#c8a96c"
        },
        "enumMember": {
            "foreground": "#d4b574"
        },
        "namespace": {
            "foreground": "#b8a6c8"
        },
        "module": {
            "foreground": "#b8a6c8"
        },
        "keyword": {
            "foreground": "#8b7ac7"
        },
        "keyword.control": {
            "foreground": "#a085d1"
        },
        "keyword.operator": {
            "foreground": "#7ba3c4"
        },
        "string": {
            "foreground": "#6aaf7f"
        },
        "string.escape": {
            "foreground": "#c8a96c"
        },
        "number": {
            "foreground": "#c8a96c"
        },
        "regexp": {
            "foreground": "#d4a574"
        },
        "comment": {
            "foreground": "#6b7280",
            "fontStyle": "italic"
        },
        "comment.documentation": {
            "foreground": "#7b8390"
        },
        "decorator": {
            "foreground": "#c8a96c"
        }
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#6b7280"
            }
        },
        {
            "name": "Documentation Comments",
            "scope": [
                "comment.block.documentation",
                "comment.line.documentation"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#7b8390"
            }
        },
        {
            "name": "Keywords and Control Flow",
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator.new",
                "keyword.other.import",
                "keyword.other.package",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#a085d1",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "punctuation.accessor",
                "punctuation.separator"
            ],
            "settings": {
                "foreground": "#7ba3c4"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#6aaf7f"
            }
        },
        {
            "name": "String Escapes",
            "scope": [
                "constant.character.escape",
                "string.regexp"
            ],
            "settings": {
                "foreground": "#c8a96c"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.other"
            ],
            "settings": {
                "foreground": "#c8a96c"
            }
        },
        {
            "name": "Functions and Methods",
            "scope": [
                "entity.name.function",
                "meta.function-call.generic",
                "support.function"
            ],
            "settings": {
                "foreground": "#6ba3d0"
            }
        },
        {
            "name": "Classes and Types",
            "scope": [
                "entity.name.class",
                "entity.name.type",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#c8a96c"
            }
        },
        {
            "name": "Variables and Properties",
            "scope": [
                "variable",
                "variable.other",
                "entity.name.variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#a8b8c8"
            }
        },
        {
            "name": "Object Properties",
            "scope": [
                "variable.other.property",
                "variable.other.object.property",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#5da6a3"
            }
        },
        {
            "name": "CSS Selectors",
            "scope": [
                "entity.name.tag.css",
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.id.css"
            ],
            "settings": {
                "foreground": "#6ba3d0"
            }
        },
        {
            "name": "CSS Properties",
            "scope": [
                "support.type.property-name.css",
                "meta.property-name.css"
            ],
            "settings": {
                "foreground": "#5da6a3"
            }
        },
        {
            "name": "HTML Tags",
            "scope": [
                "entity.name.tag.html",
                "entity.name.tag.xml"
            ],
            "settings": {
                "foreground": "#6ba3d0"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "entity.other.attribute-name.html",
                "entity.other.attribute-name.xml"
            ],
            "settings": {
                "foreground": "#c8a96c"
            }
        },
        {
            "name": "Markdown Headers",
            "scope": [
                "markup.heading",
                "entity.name.section.markdown"
            ],
            "settings": {
                "foreground": "#6ba3d0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#6aaf7f"
            }
        },
        {
            "name": "Markdown Code",
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code"
            ],
            "settings": {
                "foreground": "#c8a96c"
            }
        },
        {
            "name": "JSON Keys",
            "scope": [
                "support.type.property-name.json",
                "string.quoted.double.json"
            ],
            "settings": {
                "foreground": "#5da6a3"
            }
        },
        {
            "name": "Error and Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#d67878",
                "fontStyle": "underline"
            }
        }
    ],
