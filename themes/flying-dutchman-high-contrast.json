{
    "name": "The Flying Dutchman High Contrast",
    "author": "Created for enhanced accessibility - nautical theme lovers",
    "type": "dark",
    "semanticTokenColors": {
        "parameter.declaration": {
            "foreground": "#f4d03f"
        },
        "parameter": {
            "foreground": "#d5dbdb"
        },
        "property.declaration": {
            "foreground": "#5dddd9"
        },
        "property.defaultLibrary": {
            "foreground": "#85c1e9"
        },
        "*.defaultLibrary": {
            "foreground": "#85c1e9"
        },
        "variable.defaultLibrary": {
            "foreground": "#85c1e9"
        },
        "variable.declaration": {
            "foreground": "#aed6f1"
        },
        "variable": {
            "foreground": "#d5dbdb"
        },
        "method": {
            "foreground": "#7fb3d3"
        },
        "method.declaration": {
            "foreground": "#7fb3d3"
        },
        "method.defaultLibrary": {
            "foreground": "#5dade2"
        },
        "function": {
            "foreground": "#85c1e9"
        },
        "function.defaultLibrary": {
            "foreground": "#85c1e9"
        },
        "macro": {
            "foreground": "#7fb3d3"
        },
        "type": {
            "foreground": "#f4d03f"
        },
        "type.defaultLibrary": {
            "foreground": "#f7dc6f"
        },
        "class": {
            "foreground": "#f4d03f"
        },
        "class.defaultLibrary": {
            "foreground": "#f7dc6f"
        },
        "interface": {
            "foreground": "#f4d03f"
        },
        "enum": {
            "foreground": "#f4d03f"
        },
        "enumMember": {
            "foreground": "#f7dc6f"
        },
        "namespace": {
            "foreground": "#d5a6bd"
        },
        "module": {
            "foreground": "#d5a6bd"
        },
        "keyword": {
            "foreground": "#af7ac5",
            "fontStyle": "bold"
        },
        "keyword.control": {
            "foreground": "#bb8fce",
            "fontStyle": "bold"
        },
        "keyword.operator": {
            "foreground": "#85c1e9"
        },
        "string": {
            "foreground": "#7dcea0"
        },
        "string.escape": {
            "foreground": "#f7dc6f"
        },
        "number": {
            "foreground": "#f7dc6f"
        },
        "regexp": {
            "foreground": "#f8c471"
        },
        "comment": {
            "foreground": "#85929e",
            "fontStyle": "italic"
        },
        "comment.documentation": {
            "foreground": "#a6acaf"
        },
        "decorator": {
            "foreground": "#f7dc6f"
        }
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#85929e"
            }
        },
        {
            "name": "Documentation Comments",
            "scope": [
                "comment.block.documentation",
                "comment.line.documentation"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#a6acaf"
            }
        },
        {
            "name": "Keywords and Control Flow",
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator.new",
                "keyword.other.import",
                "keyword.other.package",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#bb8fce",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "punctuation.accessor",
                "punctuation.separator"
            ],
            "settings": {
                "foreground": "#85c1e9"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#7dcea0"
            }
        },
        {
            "name": "String Escapes",
            "scope": [
                "constant.character.escape",
                "string.regexp"
            ],
            "settings": {
                "foreground": "#f7dc6f"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.other"
            ],
            "settings": {
                "foreground": "#f7dc6f"
            }
        },
        {
            "name": "Functions and Methods",
            "scope": [
                "entity.name.function",
                "meta.function-call.generic",
                "support.function"
            ],
            "settings": {
                "foreground": "#85c1e9"
            }
        },
        {
            "name": "Classes and Types",
            "scope": [
                "entity.name.class",
                "entity.name.type",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#f4d03f"
            }
        },
        {
            "name": "Variables and Properties",
            "scope": [
                "variable",
                "variable.other",
                "entity.name.variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#d5dbdb"
            }
        },
        {
            "name": "Object Properties",
            "scope": [
                "variable.other.property",
                "variable.other.object.property",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#5dddd9"
            }
        },
        {
            "name": "CSS Selectors",
            "scope": [
                "entity.name.tag.css",
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.id.css"
            ],
            "settings": {
                "foreground": "#85c1e9"
            }
        },
        {
            "name": "CSS Properties",
            "scope": [
                "support.type.property-name.css",
                "meta.property-name.css"
            ],
            "settings": {
                "foreground": "#5dddd9"
            }
        },
        {
            "name": "HTML Tags",
            "scope": [
                "entity.name.tag.html",
                "entity.name.tag.xml"
            ],
            "settings": {
                "foreground": "#85c1e9"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "entity.other.attribute-name.html",
                "entity.other.attribute-name.xml"
            ],
            "settings": {
                "foreground": "#f4d03f"
            }
        },
        {
            "name": "Markdown Headers",
            "scope": [
                "markup.heading",
                "entity.name.section.markdown"
            ],
            "settings": {
                "foreground": "#85c1e9",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#7dcea0"
            }
        },
        {
            "name": "Markdown Code",
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code"
            ],
            "settings": {
                "foreground": "#f7dc6f"
            }
        },
        {
            "name": "JSON Keys",
            "scope": [
                "support.type.property-name.json",
                "string.quoted.double.json"
            ],
            "settings": {
                "foreground": "#5dddd9"
            }
        },
        {
            "name": "Error and Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#ff6b6b",
                "fontStyle": "underline"
            }
        }
    ],
